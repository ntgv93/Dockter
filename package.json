{
  "name": "dockter",
  "productName": "Dockter",
  "version": "0.1.0",
  "description": "Low overhead, open-source Docker log management tool",
  "scripts": {
    "start": "NODE_ENV=production node server/server.js",
    "build": "NODE_ENV=production webpack",
    "dev:hot": "webpack serve --mode development --env development & nodemon server/server.js",
    "test": "cross-env BABEL_DISABLE_CACHE=1 jest",
    "test-all": "yarn tsc && yarn build && yarn test",
    "test-e2e": "node -r @babel/register ./internals/scripts/CheckBuildsExist.js && cross-env NODE_ENV=test testcafe electron:./app ./test/e2e/HomePage.e2e.ts",
    "test-e2e-live": "node -r @babel/register ./internals/scripts/CheckBuildsExist.js && cross-env NODE_ENV=test testcafe --live electron:./app ./test/e2e/HomePage.e2e.ts",
    "test-watch": "yarn test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oslabs-beta/Dockter.git"
  },
  "author": {
    "name": "BANKS",
    "email": "",
    "url": "https://dockter.io"
  },
  "contributors": [
    {
      "name": "Ben Brower",
      "email": "",
      "url": "https://github.com/bbrower21"
    },
    {
      "name": "Anup Ramdass",
      "email": "",
      "url": "https://github.com/anrax"
    },
    {
      "name": "Nancy Kousholt",
      "email": "",
      "url": "https://github.com/noodlemonkey"
    },
    {
      "name": "Kyle Whang",
      "email": "",
      "url": "https://github.com/shaisle"
    },
    {
      "name": "Samuel Kim",
      "email": "",
      "url": "https://github.com/samuy"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oslabs-beta/Dockter/issues"
  },
  "keywords": [
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "homepage": "https://github.com/oslabs-beta/Dockter/README.md",
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "setupFiles": [
      "./internals/scripts/CheckBuildsExist.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "nodemon": "^2.0.7",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "ansi-to-html": "^0.6.14",
    "dockerode": "^3.2.1",
    "dompurify": "^2.2.6",
    "express": "^4.17.1",
    "html-react-parser": "^1.1.1",
    "influx": "^5.6.3",
    "path": "^0.12.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-infinite-scroll-component": "^5.1.0",
    "react-table": "^7.6.0",
    "tailwindcss": "^1.9.2"
  }
}
